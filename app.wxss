@import "./styles/iconfont.wxss";

page,
text,
image,
swiper,
swiper-item,
view,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 
    原生wxss定义变量
*/
page {
  --themeColor: #eb4450;
  /* 
        统一字体大小
        加入设计稿是375px
        1px = 2rpx
    */
  font-size: 28rpx;
}

image {
  width: 100%;
}
